<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.czStudio.mapper.TeamCompetitionMapper">
    <select id="selectCompetitionAll" resultType="com.czStudio.domain.TeamCompetition">
        select * from team_competition
    </select>
    <select id="selectCompetitionById" resultType="com.czStudio.domain.TeamCompetition" parameterType="int">
        select * from team_competition where id=#{id}
    </select>
</mapper>